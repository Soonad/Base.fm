import Nat

T Path (len : Nat)
| pe                            : Path(zero)
| pl(len: Nat, pred: Path(len)) : Path(succ(len))
| pr(len: Nat, pred: Path(len)) : Path(succ(len))

path_len(len: Nat, path: Path(len)) : Nat
  case path
  | pe => zero
  | pl => succ(path_len(path.len, path.pred))
  | pr => succ(path_len(path.len, path.pred))
  : Nat
