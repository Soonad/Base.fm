// String.fm
// =========
// 
// A list of Unicode characters.

import List

// Definition
// ----------

Char : Type
  Number

String : Type
  List(Char)

// Operations
// ----------

Output : Type
  [tag : Number, String]

print(str : String) : Output
  [0x53484f57, str]

string.test : Output
  let str = "Hello, world!"
  let str = reverse(_ str)
  print(str)
