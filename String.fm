// String.fm defines a string type as a list of characters
//

import List

// :::::::::::::::::::::::::::::::::::::::::::::::::::::::::
// ::                     Definition                      ::
// :::::::::::::::::::::::::::::::::::::::::::::::::::::::::

Char : Type
  Number

String : Type
  List(Char)

Output : Type
  [tag : Number, String]

print(str : String) : Output
  [0x53484f57, str]

string.test : Output
  let str = "Hello, world!"
  let str = reverse(~Char, str)
  print(str)
