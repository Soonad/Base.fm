import Algebra.Magma

T Quasigroup {A : Type, s : Setoid(A)}
| quasigroup
  { f           : Op2(A)
  , latinSquare : LatinSquare(A,s,f)
  }

T LoopQuasigroup {A : Type, s : Setoid(A)}
| loop_quasigroup
  { f           : Op2(A)
  , e           : A
  , identity    : Identity(A,s,f,e)
  , latinSquare : LatinSquare(A,s,f)
  }
